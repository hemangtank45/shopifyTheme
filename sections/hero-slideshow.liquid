{{ 'hero-slideshow.css' | asset_url | stylesheet_tag }}

{% if section.blocks.size > 0 %}
{% assign full = section.settings.full %}
<div
  class="{% assign full = section.settings.full %}{% assign no_spacing = section.settings.spacing_both_ends %}{% if section.settings.full %}lg-full-width{% else %}page-width{% endif %}{% if section.settings.spacing_both_ends %} spacing_enabled{% endif %} hero-slider"
  data-section-id="{{ section.id }}" data-section-type="slideshow-section"
  data-autoPlay="{{ section.settings.autoplay }}" data-animation-speed="{{ section.settings.slideshow_speed}}">
  <div class="{{ section.settings.slider_width }} lg-main-row">
  <div class="hero-main-slideshow">
  <div class="hero-slideshow-image">
    <div
      class="classic-container swiper-container {% if section.settings.arrows %}  {{ section.settings.nav_arrow_position }} {{ section.settings.center_nav_arrow_alignment }} {% endif %}"
      data-slider="{{ section.settings.slider_width }}">
      <div id="home-slider-{{ section.id }}">
        <div class="swiper-wrapper">
          {% for block in section.blocks %}
          {%- if block.settings.text_alignment == 'text-start' -%}
          {%- assign style = '1' -%}
          {%- elsif block.settings.text_alignment == 'text-end' -%}
          {%- assign style = '2' -%}
          {%- else -%}
          {%- assign style = '3' -%}
          {%- endif -%}
          <div class="slider_style_{{style}} slider-{{ block.id }}-list swiper-slide">
            {% include 'slider-data' %}
          </div>
          {% endfor %}
        </div>

     
        {% if section.settings.arrows %}
        <div class="swiper-{{ section.id}}-arrows swiper-arrows">
          <div class="swiper-button-next classic-arrows" id="swiper-button-next-{{section.id}}">{% render 'icon-right' %}</div>
          <div class="swiper-button-prev classic-arrows" id="swiper-button-prev-{{section.id}}">{% render 'icon-left' %}</div>
        </div>
        {% endif %}
        {% if section.settings.dots -%}
        <div class="swiper-pagination swiper-pagination--{{ section.settings.carousel_pagination_alignment }} "
          id="swiper-pagination-{{section.id}}"></div>
        {% endif %}
      </div>
    </div>
    </div>

       {% if section.settings.slider_width == 'second_slider' %}
        <div class="hero-slideshow-image2">
          <a {% if section.settings.first-banner-btn-url %} href="{{ section.settings.first-banner-btn-url }}" {%else %} role="link" aria-disabled="true" {% endif %}>
              <div class="banner-hover-img">
                <div class="banner-slider-img">
                    {% if section.settings.first-banner-image != blank %}
                    <div class="media media--transparent ratio" style="--ratio-percent: {{ section.settings.image-ratio1 }}%">
                      {{ section.settings.first-banner-image | image_url: width: 1920 | image_tag : loading: 'lazy', class: 'w-100' }}
                    </div>
                    {% else %}
                    <div class="media media--transparent ratio" style="--ratio-percent: {{ section.settings.image-ratio1 }}%">
                      {{ 'collection-4' | placeholder_svg_tag: 'placeholder-svg placeholder' }}
                    </div>
                    {% endif %}
                 </div>
              
                  <div class="hero-slider-text hero-slide-text--{{ section.settings.text_alignment1 }} hero-slider-block--{{ section.settings.text_block_alignment1 }}">
                    {% if section.settings.first-banner-heading-text != blank %}
                    <div class="banner-{{ section.id }} theme_main_heading h2">{{ section.settings.first-banner-heading-text }}</div>
                    {% endif %}
          
                   {% if section.settings.first-banner-subheading-text != blank %}
                    <div class="banner-{{ section.id }} theme_main_subheading">{{ section.settings.first-banner-subheading-text }}</div>
                    {% endif %}
         
          
                    <div class="offer_text">
                      {% if section.settings.offer_text1 != blank %}
                        <span class="theme_main_subheading offer_subheading">{{ section.settings.offer_text1 }}</span>
                      {% endif %}
                       {% if section.settings.offer_text1-1 != blank %}
                        <span class="theme_main_heading h2">{{ section.settings.offer_text1-1 }}</span>
                      {% endif %}
                    </div>
                  </div>
                </div>
            </a>
        </div>
      {% endif %}


