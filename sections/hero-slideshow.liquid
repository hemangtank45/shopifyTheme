{% style %}
  .hero-slideshow-image2 .theme_main_heading,
  .hero-slideshow-image2 .theme_main_subheading {
    color: {{ section.settings.banner_text_color }};
  }
  .slider_style_1.swiper-slide.swiper-slide-active .slide-sub-heading-2,
  .slider_style_2.swiper-slide.swiper-slide-active .slide-sub-heading-2,
  .slider_style_3.swiper-slide.swiper-slide-active .slide-sub-heading-2 {
    animation: 1s ease 0s normal none 1 running fadeInLeft;
    animation-duration: 1s;
  }
  .slider_style_1.swiper-slide.swiper-slide-active .slide-heading,
  .slider_style_2.swiper-slide.swiper-slide-active .slide-heading,
  .slider_style_3.swiper-slide.swiper-slide-active .slide-heading {
    animation: 1s ease 0s normal none 1 running fadeInUp;
    animation-duration: 2s;
  }
  .slider_style_1.swiper-slide.swiper-slide-active .slide-text,
  .slider_style_2.swiper-slide.swiper-slide-active .slide-text,
  .slider_style_3.swiper-slide.swiper-slide-active .slide-text {
    animation: 1s ease 0s normal none 1 running fadeInLeft;
    animation-duration: 3s;
  }
  .slider_style_1.swiper-slide.swiper-slide-active .multiple-buttons,
  .slider_style_2.swiper-slide.swiper-slide-active .multiple-buttons,
  .slider_style_3.swiper-slide.swiper-slide-active .multiple-buttons {
    animation: 1s ease 0s normal none 1 running fadeInUp;
    animation-duration: 2s;
  }
  
  .slider-content--top-left {
    justify-content: flex-start;
  }
  .slider-content--top-center {
    justify-content: center;
  }
  .slider-content--top-right {
    justify-content: flex-end;
  }
  .slider-content--middle-left {
    justify-content: flex-start;
  }
  .slider-content--middle-center {
    justify-content: center;
  }
  .slider-content--middle-right {
    justify-content: flex-end;
  }
  .slider-content--bottom-left {
    justify-content: flex-start;
  }
  .slider-content--bottom-center {
    justify-content: center;
  }
  .slider-content--bottom-right {
    justify-content: flex-end;
  }
  .home-slideshow-section .swiper-pagination--left_pagination {
    text-align: left;
  }
  .home-slideshow-section .swiper-pagination--center_pagination {
    text-align: center;
  }
  .home-slideshow-section .swiper-pagination--right_pagination {
    text-align: right;
  }
  .hero-main-slideshow{
    display: inline-block;
    width: 100%;
    position: relative;
    vertical-align: middle;
  }
  {% if section.settings.slider_width == 'fourth_slider' %}
    .hero-slideshow-image {
      max-width: 67%;
      width: 100%;
      float: left;
    }
    .hero-slideshow-image2 {
      max-width: 33%;
      width: 100%;
      float: left;
      }
  {% endif %}
  {% if section.settings.slider_width == 'second_slider' %}
    .hero-slideshow-image {
      max-width: 66.8%;
      width: 100%;
      float: left;
    }
    .hero-slideshow-image2 {
      max-width: 33.2%;
      width: 100%;
      float: left;
    }
      .hero-slideshow-image2 .offer_text{
        margin-top: 15px;
      }
      .home-slideshow-section .second_slider .classic-container .swiper-wrapper .slider-content{
        padding: 0 15px;
      }
    .swiper-slide.swiper-slide-active .hero-slideshow-image .slideshow-img {
      animation: 1s ease 0s normal none 1 running fadeInUp;
      animation-duration: 2s;
    }
    .swiper-slide.swiper-slide-active .hero-slideshow-image2 .slideshow-img {
      animation: 1s ease 0s normal none 1 running fadeInDown;
      animation-duration: 2s;
    }
    {% endif %}
  
    {% if section.settings.slider_width == 'third_slider' %}
    .hero-slideshow-image {
      max-width: 50%;
      width: 100%;
      float: left;
    }
    .hero-slideshow-image2 {
      max-width: 50%;
      width: 100%;
      float: left;
      }
    .swiper-slide.swiper-slide-active .hero-slideshow-image .slideshow-img {
      animation: 1s ease 0s normal none 1 running fadeInUp;
      animation-duration: 2s;
    }
    .swiper-slide.swiper-slide-active .hero-slideshow-image2 .slideshow-img {
      animation: 1s ease 0s normal none 1 running fadeInDown;
      animation-duration: 2s;
    }
  {% endif %}
  #shopify-section-{{section.id}}.home-slideshow-section .classic-container { 
    padding-bottom: 0;
    height: {{ section.settings.slider_height }}px;
    border-radius: var(--radius_img);
  }
  #shopify-section-{{section.id}}.home-slideshow-section .classic-container.swiper-container [class*="swiper-container"] > .swiper-wrapper,
  #shopify-section-{{section.id}}.home-slideshow-section .classic-container.swiper-container [class*="swiper-container"] > .swiper-wrapper > .swiper-slide,
  #shopify-section-{{section.id}}.home-slideshow-section .classic-container.swiper-container [class*="swiper-container"] > .swiper-wrapper > .swiper-slide  img { 
    border-radius: inherit;
    height: 100% !Important;
  }
  #shopify-section-{{section.id}}.home-slideshow-section .classic-container.swiper-container .swiper-arrows .classic-arrows {  
    background: {{ settings.theme_nav_bg }}; 
    opacity: 0;
    -moz-opacity: 0;
    -o-opacity: 0;
    -webkit-opacity: 0;
  }
   #shopify-section-{{section.id}}.home-slideshow-section:hover .classic-container.swiper-container .swiper-arrows .classic-arrows {  
    opacity: 1;
    -moz-opacity: 1;
    -o-opacity: 1;
    -webkit-opacity: 1;
  }

  #shopify-section-{{section.id}}.home-slideshow-section .classic-container.swiper-container .swiper-arrows .classic-arrows[class*="swiper-button-"]:after, .classic-arrows {   
    stroke: {{ settings.theme_nav_icon }};
  }
  #shopify-section-{{section.id}}.home-slideshow-section .classic-container.swiper-container .swiper-arrows .classic-arrows:hover { 
    background: {{ settings.theme_nav_hr_bg }}; 
  }
  #shopify-section-{{section.id}}.home-slideshow-section .classic-container.swiper-container .swiper-arrows .classic-arrows:hover[class*="swiper-button-"]:after, .classic-arrows:hover { 
    stroke: {{ settings.theme_nav_hr_icon }};
  }
  #shopify-section-{{section.id}}.home-slideshow-section .classic-container.swiper-container .swiper-pagination-bullet { 
    background: {{ settings.theme_dots }}; 
    transition: all 0.5s ease-in-out;
    opacity: 1;
  }  
  #shopify-section-{{section.id}}.home-slideshow-section .classic-container.swiper-container .swiper-arrows .classic-arrows p{
    display: none;
  }
  #shopify-section-{{section.id}}.home-slideshow-section .classic-container.swiper-container .swiper-pagination-bullet.swiper-pagination-bullet-active { 
    background: {{ settings.theme_dots_active }};
    width: 40px;
    transition: all 0.5s ease-in-out;
    opacity: 1;
  }
  
  {% for block in section.blocks %}
    .home-slideshow-section .slider-{{ block.id }}-list .slide-button{
      border: 1px dashed {{ block.settings.slide_btn_bg }};
      color: {{ block.settings.slide_btn_bg }};
      background-color: transparent;
       margin-top: 25px;
    }
    .home-slideshow-section .slider-{{ block.id }}-list .slide-button.secondary { 
      background-color: {{ block.settings.slide_btn_bg }};
      color:{{ block.settings.slide_btn_text }};
      margin-top: 25px;
      border: none !important;
    }
    .home-slideshow-section .slider-{{ block.id }}-list .slide-button:hover{
      background:{{ block.settings.slide_btn_hr_bg }};
      color:{{ block.settings.slide_btn_hr_text }};
      border: 1px dashed {{ block.settings.slide_btn_hr_bg }};
    }
    .home-slideshow-section .slider-{{ block.id }}-list .slide-button.secondary:hover { 
      background:{{ block.settings.slide_btn_hr_bg }};
      color:{{ block.settings.slide_btn_hr_text }};
    }
    .home-slideshow-section .classic-container .slider-{{ block.id }}-list .slider-content [class*="slide_"] { 
      position: relative;
      z-index: 1;
    }
    .home-slideshow-section .classic-container .slider-{{ block.id }}-list .slider-content [class*="slide_"]::after { 
      content: "";
      opacity: {{ block.settings.overlay | divided_by: 100.0 }};
      background-color: {{ block.settings.slide_content_bg }};
      height: 100%;
      width: 100%;
      right: 0;
      top: 0;
      bottom: 0;
      left: 0;
      position: absolute;
      z-index: -1;
    }
